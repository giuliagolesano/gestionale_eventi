<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="it.unibo.controller.Op10">
    <children>
        <Label fx:id="operationLabel" text="Inserisci i Dati del nuovo Cliente Bloccato"/>
        <TextField fx:id="idCliente" promptText="IdClente"/>
        <TextField fx:id="nome" promptText="Nome"/>
        <DatePicker fx:id="Data" promptText="Data Comportamento Illecito"/>
        <TextField fx:id="descrizioneComportamento" promptText="Descrizione"/>
        <Button text="Esegui" onAction="#executeOperation"/>
    </children>
</VBox>
