<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="it.unibo.controller.Op16">
    <children>
        <Label fx:id="operationLabel" text="Inserisci i Dati modificati dell'Evento"/>
        <TextField fx:id="Tema" promptText="Nuovo Tema"/>
        <DatePicker fx:id="Data" promptText="Data Evento da cambiare"/>
        <Button text="Esegui" onAction="#executeOperation"/>
    </children>
</VBox>
